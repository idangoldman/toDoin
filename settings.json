{
    "assets": {
        "stylesheet": {
            "bower": [
                "./bower_components/normalize-css/normalize.css"
            ],
            "layout": [
                "vendors/css/normalize.css",
                "style.css"
            ]
        },
        "javascript": {
            "bower": [
                "./bower_components/requirejs/require.js",
                "./bower_components/text/text.js",
                "./bower_components/jquery/dist/jquery.js",
                "./bower_components/underscore/underscore.js",
                "./bower_components/backbone/backbone.js",
                "./bower_components/backbone.localStorage/backbone.localStorage.js"
            ],
            "require": {
                "config": "require-config.js",
                "path": "vendors/js/require.js",
                "files": {
                    "text": "vendors/js/text",
                    "jquery": "vendors/js/jquery",
                    "underscore": "vendors/js/underscore",
                    "backbone": "vendors/js/backbone",
                    "backboneLocalstorage": "vendors/js/backbone.localStorage"
                }
            },
            "main": "script.js"
        }
    },
    "compass": {
        "config_file": "./config.rb",
        "css": "./css",
        "sass": "./app/"
    },
    "layout": {
        "title": "ToDoin"
    }
}
{
  "name": "ToDoin",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "yet another todo list",
  "homepage_url": "http://todo.in",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Todo List",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "cookies"
  ]
}
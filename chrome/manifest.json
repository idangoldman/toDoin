{
  "name": "ToDoin",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "Make your life easier.",
  "homepage_url": "http://blah.co.il/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "ToDoin",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "cookies",
    "fontSettings"
  ]
}
{
    "name": "ToDoin",
    "version": "1.4.0",
    "manifest_version": 2,
    "description": "Make your life easier.",
    "homepage_url": "http://blah.co.il/todo",
    "default_locale": "en",
    "offline_enabled": true,
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon16.png",
        "default_title": "ToDoin",
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "cookies",
        "fontSettings"
    ]
}